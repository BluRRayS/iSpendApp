
@{
    ViewData["Title"] = "Login";
}
<div class="InlogBox">
    <div class="panel-group">
        <div class=" panel panel-default text-center">
            <div class="panel-heading"><h1>Login</h1></div>
            <div class="row">
                <div class="text-center">
                    <img src="/images/AccountIcon.svg" style="width: 150px; height: 100%;" /><br />
                    <form>
                        <input class="form-control" type="text" placeholder="Username" style="margin: auto" /><br />
                        <input class="form-control" type="password" placeholder="Password" style="margin: auto" /><br />
                        <input class="btn" type="submit" placeholder="Login" style="margin: auto" /><br />
                    </form>
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>






