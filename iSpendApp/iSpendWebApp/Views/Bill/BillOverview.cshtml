@model IEnumerable<iSpendWebApp.Models.Bill.BillViewModel>

@{
    ViewData["Title"] = "BillOverview";
}

<h2>BillOverview</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="container">

    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <div class="col-sm-4">
                @{
                    Html.RenderPartial("BillPartial", item);
                }
            </div>

        }
    }

</div>